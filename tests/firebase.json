{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/about",
        "destination": "/pages/about.html"
      },
      {
        "source": "**/contact",
        "destination": "/pages/contact.html"
      },
      {
        "source": "/",
        "destination": "/pages/index.html"
      }
    ]
  }
}
